<application>
  <component name="ParameterNameHintsSettings">
    <blacklists>
      <blacklist language="ECMAScript 6">
        <added pattern="setTimeout(handler,timeout,arguments)" />
        <added pattern="Array.map(callbackfn,thisArg)" />
        <added pattern="parseFloat(string)" />
        <added pattern="Math.floor(x)" />
      </blacklist>
      <blacklist language="PHP">
        <added pattern="\Illuminate\Database\Eloquent\Concerns\HasEvents.observe(classes)" />
        <added pattern="\SilverStripe\Core\Injector\Injectable.create(args)" />
        <added pattern="\SilverStripe\ORM\FieldType\DBField.create_field(spec,value,name,args)" />
        <added pattern="\file_get_contents(filename,use_include_path,context,offset,maxlen)" />
        <added pattern="\compact(varname,_)" />
        <added pattern="\Illuminate\Database\Eloquent\Builder.where(column,operator,value,boolean)" />
        <added pattern="\flash(message,level)" />
        <added pattern="\Illuminate\Routing\Redirector.route(route,parameters,status,headers)" />
        <added pattern="\sprintf(format,args,_)" />
      </blacklist>
    </blacklists>
  </component>
</application>