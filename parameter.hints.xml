<application>
  <component name="ParameterNameHintsSettings">
    <blacklists>
      <blacklist language="ECMAScript 6">
        <added pattern="setTimeout(handler,timeout,arguments)" />
        <added pattern="Array.map(callbackfn,thisArg)" />
        <added pattern="parseFloat(string)" />
        <added pattern="Math.floor(x)" />
      </blacklist>
      <blacklist language="PHP">
        <added pattern="\Illuminate\Database\Eloquent\Concerns\HasEvents.observe(classes)" />
        <added pattern="\SilverStripe\Core\Injector\Injectable.create(args)" />
        <added pattern="\SilverStripe\ORM\FieldType\DBField.create_field(spec,value,name,args)" />
        <added pattern="\file_get_contents(filename,use_include_path,context,offset,maxlen)" />
        <added pattern="\compact(varname,_)" />
        <added pattern="\Illuminate\Database\Eloquent\Builder.where(column,operator,value,boolean)" />
        <added pattern="\flash(message,level)" />
        <added pattern="\Illuminate\Routing\Redirector.route(route,parameters,status,headers)" />
        <added pattern="\sprintf(format,args,_)" />
        <added pattern="\array_column(array,column,index_key)" />
        <added pattern="\abort_if(boolean,code,message,headers)" />
        <added pattern="\Illuminate\Http\Concerns\InteractsWithInput.filled(key)" />
        <added pattern="\Illuminate\Database\Eloquent\Model.update(attributes,options)" />
        <added pattern="\Carbon\Carbon.createFromFormat(format,time,tz)" />
        <added pattern="\Tightenco\Collect\Support\Collection.get(key,default)" />
        <added pattern="\explode(delimiter,string,limit)" />
        <added pattern="\Illuminate\Database\Schema\Blueprint.increments(column)" />
        <added pattern="\Illuminate\Database\Schema\Blueprint.unsignedInteger(column,autoIncrement)" />
        <added pattern="\Illuminate\Database\Schema\Blueprint.string(column,length)" />
        <added pattern="\Illuminate\Database\Schema\Blueprint.float(column,total,places)" />
        <added pattern="\Illuminate\Database\Schema\Blueprint.timestamp(column,precision)" />
        <added pattern="\Illuminate\Database\Schema\Blueprint.text(column)" />
        <added pattern="\Illuminate\Database\Schema\Blueprint.index(columns,name,algorithm)" />
        <added pattern="\Illuminate\Database\Schema\Blueprint.foreign(columns,name)" />
        <added pattern="\Illuminate\Mail\Mailable.view(view,data)" />
        <added pattern="\Illuminate\Mail\Mailable.text(textView,data)" />
        <added pattern="\throw_unless(condition,exception,parameters)" />
        <added pattern="\Illuminate\Support\Collection.implode(value,glue)" />
      </blacklist>
    </blacklists>
  </component>
</application>